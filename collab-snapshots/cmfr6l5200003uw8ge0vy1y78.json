{
  "index.js/index.js": "import app from \"./app.js\";\r\nconst port = 3001\r\n\r\napp.listen(port, () => {\r\n  console.log(`App listening on port ${port}`)\r\n})",
  "controllers/pokeController.js/pokeController.js": "import { catchAsyncErrors } from \"../middlewares/catchAsyncError.js\";\r\nimport Errorhandler from \"../middlewares/error.js\";\r\n\r\nexport const getPokemon = catchAsyncErrors(async (req, res, next) => {\r\n  const { name } = req.params;\r\n\r\n  const response = await fetch(`https://pokeapi.co/api/v2/pokemon/${name}`);\r\n\r\n  if (!response.ok) {\r\n    return next(new Errorhandler(`Pokemon ${name} not found!`, 404));\r\n  }\r\n\r\n  const data = await response.json();\r\n  const image = data.sprites.other.home.front_default;\r\n  res.status(200).json({\r\n    success: true,\r\n    name: data.name,\r\n    image,\r\n    types: data.types.map(t => t.type.name),\r\n    stats: data.stats.map(stat => ({\r\n      name: stat.stat.name,\r\n      value: stat.base_stat\r\n    }))\r\n  });\r\n});\r\n",
  "routers/pokeRoutes.js/pokeRoutes.js": "import express from 'express'\r\nimport { getPokemon } from '../controllers/pokeController.js'\r\n\r\nconst router = express.Router();\r\n\r\nrouter.get(\"/get/:name\", getPokemon);\r\n\r\nexport default router;",
  "middlewares/catchAsyncError.js/catchAsyncError.js": "export const catchAsyncErrors = (theFunction) => {\r\n  return (req, res, next) => {\r\n    Promise.resolve(theFunction(req, res, next)).catch(next);\r\n  };\r\n}",
  "middlewares/error.js/error.js": "class ErrorHandler extends Error {\r\n  constructor(message, statusCode) {\r\n    super(message);\r\n    this.statusCode = statusCode;\r\n  }\r\n}\r\n\r\nexport default ErrorHandler;",
  "public/index.html/index.html": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n  <meta charset=\"UTF-8\" />\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\r\n  <title>Pokedex - Dark Theme</title>\r\n  <!-- Bootstrap Darkly theme -->\r\n  <link href=\"https://cdn.jsdelivr.net/npm/bootswatch@5.3.3/dist/darkly/bootstrap.min.css\" rel=\"stylesheet\" />\r\n  <style>\r\n    body {\r\n      background: linear-gradient(to right, #0f2027, #203a43, #2c5364);\r\n      color: #f1f1f1;\r\n      font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;\r\n      padding-top: 2rem;\r\n      min-height: 100vh;\r\n    }\r\n\r\n    h1 {\r\n      font-size: 2.75rem;\r\n      font-weight: bold;\r\n      text-shadow: 0 0 10px rgba(255, 255, 255, 0.1);\r\n    }\r\n\r\n    .input-group {\r\n      max-width: 500px;\r\n      margin: auto;\r\n    }\r\n\r\n    input.form-control {\r\n      background-color: #1c1c1c;\r\n      color: #ffffff;\r\n      border: 1px solid #444;\r\n      transition: all 0.3s ease;\r\n    }\r\n\r\n    input.form-control:focus {\r\n      box-shadow: 0 0 0 0.25rem rgba(33, 133, 208, 0.4);\r\n      background-color: #222;\r\n      color: #fff;\r\n    }\r\n\r\n    .btn-primary {\r\n      background: linear-gradient(145deg, #1a73e8, #005bb5);\r\n      border: none;\r\n      font-weight: bold;\r\n      box-shadow: 0 4px 12px rgba(33, 133, 208, 0.3);\r\n      transition: background 0.3s ease;\r\n    }\r\n\r\n    .btn-primary:hover {\r\n      background: linear-gradient(145deg, #1761a0, #004b99);\r\n    }\r\n\r\n    .card {\r\n      background: #1a1a1a;\r\n      color: #e0e0e0;\r\n      border-radius: 15px;\r\n      box-shadow: 0 0 20px rgba(255, 255, 255, 0.05);\r\n      transition: transform 0.3s ease;\r\n    }\r\n\r\n    .card:hover {\r\n      transform: scale(1.02);\r\n    }\r\n\r\n    .card-title {\r\n      font-size: 1.75rem;\r\n      font-weight: bold;\r\n      text-shadow: 0 0 5px rgba(255, 255, 255, 0.1);\r\n    }\r\n\r\n    img.img-fluid {\r\n      border-radius: 12px;\r\n      box-shadow: 0 0 15px rgba(33, 133, 208, 0.3);\r\n      transition: transform 0.3s ease;\r\n    }\r\n\r\n    img.img-fluid:hover {\r\n      transform: scale(1.05);\r\n    }\r\n\r\n    .list-group-item {\r\n      background-color: #292929;\r\n      color: #fff;\r\n      border: none;\r\n      font-weight: 500;\r\n      border-bottom: 1px solid #333;\r\n    }\r\n\r\n    .list-group-item:last-child {\r\n      border-bottom: none;\r\n    }\r\n\r\n    .text-danger {\r\n      font-weight: bold;\r\n      color: #ff6b6b !important;\r\n    }\r\n\r\n    .text-muted {\r\n      font-style: italic;\r\n      color: #aaa !important;\r\n    }\r\n  </style>\r\n\r\n</head>\r\n\r\n<body>\r\n  <div class=\"container py-5 text-center\">\r\n    <h1 class=\"mb-4\">üïπÔ∏è Dark Pok√©dex</h1>\r\n\r\n    <div class=\"input-group justify-content-center mb-4\">\r\n      <input id=\"pokemonName\" type=\"text\" class=\"form-control w-auto\" placeholder=\"Enter Pok√©mon name\" />\r\n      <button class=\"btn btn-primary ms-2\" onclick=\"fetchPokemon()\">Search</button>\r\n    </div>\r\n\r\n    <div id=\"result\"></div>\r\n  </div>\r\n\r\n  <script>\r\n    function escapeHTML(str) {\r\n      return str.replace(/[&<>'\"]/g, tag => ({\r\n        '&': '&amp;',\r\n        '<': '&lt;',\r\n        '>': '&gt;',\r\n        \"'\": '&#39;',\r\n        '\"': '&quot;'\r\n      }[tag]));\r\n    }\r\n\r\n    async function fetchPokemon() {\r\n      const nameInput = document.getElementById(\"pokemonName\");\r\n      const name = nameInput.value.trim().toLowerCase();\r\n      const resultDiv = document.getElementById(\"result\");\r\n      resultDiv.innerHTML = \"\";\r\n\r\n      if (!name) {\r\n        resultDiv.innerHTML = \"<div class='text-danger'>Please enter a Pok√©mon name.</div>\";\r\n        return;\r\n      }\r\n\r\n      resultDiv.innerHTML = \"<div class='text-muted'>Loading...</div>\";\r\n\r\n      try {\r\n        const res = await fetch(`http://localhost:3001/pokemon/get/${name}`);\r\n\r\n        if (!res.ok) {\r\n          const errorText = await res.text();\r\n          throw new Error(errorText || \"Pokemon not found.\");\r\n        }\r\n\r\n        const data = await res.json();\r\n\r\n        if (!data.success) {\r\n          resultDiv.innerHTML = `<div class='text-danger'>Error: ${escapeHTML(data.message || 'Pokemon not found!')}</div>`;\r\n          return;\r\n        }\r\n\r\n        const { image, types, stats } = data;\r\n\r\n        resultDiv.innerHTML = `\r\n          <div class=\"card mx-auto mb-4\" style=\"max-width: 400px;\">\r\n            <div class=\"card-body\">\r\n              <h2 class=\"card-title\">${escapeHTML(data.name.toUpperCase())}</h2>\r\n              <img src=\"${image}\" alt=\"${escapeHTML(name)}\" class=\"img-fluid mb-3\" />\r\n              <p><strong>Type:</strong> ${types.map(escapeHTML).join(', ')}</p>\r\n              <h5>Stats:</h5>\r\n              <ul class=\"list-group mb-3\">\r\n                ${stats.map(stat => `<li class=\"list-group-item d-flex justify-content-between\">${escapeHTML(stat.name)}<span>${stat.value}</span></li>`).join(\"\")}\r\n              </ul>\r\n            </div>\r\n          </div>\r\n        `;\r\n      } catch (error) {\r\n        console.error(error);\r\n        resultDiv.innerHTML = \"<div class='text-danger'>Failed to fetch Pok√©mon data.</div>\";\r\n      }\r\n\r\n      nameInput.value = \"\";\r\n    }\r\n  </script>\r\n</body>\r\n\r\n</html>",
  "app.js/app.js": "import cookieParser from \"cookie-parser\";\r\nimport express from \"express\";\r\nimport pokeRouter from \"./routers/pokeRoutes.js\";\r\nimport path from \"path\";\r\n\r\nconst app = express();\r\n\r\napp.use(cookieParser());\r\napp.use(express.json());\r\napp.use(express.urlencoded({ extended: true }));\r\napp.use(express.static(path.resolve('./public'))); // Serves static files from /public\r\n\r\napp.use(\"/pokemon\", pokeRouter);\r\n\r\nexport default app;\r\n",
